<nav>
    <a href="/">Home</a>
    <% if (user) { %>
      <a href="/auth/logout">Logout</a>
      <% if (user.role === 'admin') { %>
        <a href="/portfolio/admin">Admin</a>
      <% } else if (user.role === 'editor') { %>
        <a href="/portfolio/editor">Editor Panel</a>
      <% } %>
    <% } else { %>
      <a href="/auth/login">Login</a>
    <% } %>
</nav>
